        <section class="section">
            <% if(messages.warning){ %>
                <div class="container" id="warning">
                    <article class="message is-danger">
                        <div class="message-header">
                            <p>Attention</p>
                            <button class="delete" aria-label="delete" onclick="removeWarning()"></button>
                        </div>
                        <div class="message-body">
                            <%= messages.warning %>
                        </div>
                    </article>
                </div>
            <% } %>
            <% if(messages.info){ %>
                <div class="container" id="info">
                    <article class="message is-info">
                        <div class="message-header">
                            <p>Information</p>
                            <button class="delete" aria-label="delete" onclick="removeInfo()"></button>
                        </div>
                        <div class="message-body">
                            <%= messages.info %>
                        </div>
                    </article>
                </div>
            <% } %>
            <div class="container">
                <form action="/login" method="POST" id="loginForm">
                    <div class="field">
                        <label class="label">Nom d'utilisateur</label>
                        <div class="control">
                            <input class="input" type="text" autocomplete="username" placeholder="Nom d'utilisateur" name="username" id="username">
                        </div>
                    </div>
                    <div class="field">
                        <label class="label">Mot de passe</label>
                        <div class="control">
                            <input class="input" type="password" autocomplete="current-password" placeholder="********" name="password" id="password">
                        </div>
                    </div>
                    <div class="field is-grouped">
                        <div class="control">
                            <input type="submit" class="button is-info" id="btnLogin"></input>
                        </div>
                        <div class="control">
                            <input type="reset" class="button is-text" id="btnReset"></input>
                        </div>
                    </div>
                </form>
            </div>
        </section>